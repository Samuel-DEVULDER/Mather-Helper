program NQueen;uses  Backtracking;procedure main;  const    N = 8;  var    P: array[1..N] of integer;    i, j, k, r: integer;  begin    for i:= 1 to N do       P[i]:= i;    for i:= 1 to N do    begin        k:= P[i];        j:= i-1 + Choice(N-i+1);        P[i]:= P[j];        for r:= i-1 downto 1 do            if (P[r] - r = P[i] - i) or                (P[r] + r = P[i] + i) then                Backtrack;          P[j]:= k    end;    for i:= 1 to N do        write(P[i]:1,' ');    writeln;    Backtrack;end; begin    InitBacktracking;    main;end.